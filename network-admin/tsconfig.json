{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "../resources/js/Components/breadcrumb-nav.tsx", "../resources/js/Components/command-menu.tsx", "../resources/js/Components/connections-table.tsx", "../resources/js/Components/nav-items.tsx", "../resources/js/Components/navbar.tsx", "../resources/js/Components/network-stats.tsx", "../resources/js/Components/notification-list.tsx", "../resources/js/Components/overview.tsx", "../resources/js/Components/recent-activity.tsx", "../resources/js/Components/theme-provider.tsx"],
  "exclude": ["node_modules"]
}
